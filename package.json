{
  "name": "writers-mark-react",
  "homepage": "https://github.com/FrancoisChabot/writers-mark-react#readme",
  "version": "0.6.0",
  "description": "React Renderer for writers-mark",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "writers-mark": "^0.6.1",
    "writers-mark-dom": "^0.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.15",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/testing-library__react": "^10.2.0",
    "codecov": "^3.8.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "test": "jest --collectCoverage",
    "coverage": "codecov",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\"",
    "format": "prettier --write \"src/**/*.tsx\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format"
  },
  "files": [
    "lib/**/*"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FrancoisChabot/writers-mark-react.git"
  },
  "author": "Francois Chabot",
  "license": "ISC"
}
